{
    "name": "hybridauth/hybridauth",
    "description": "PHP Social Authentication Library For Signin and Signup",
    "keywords": ["oauth", "openid", "authentication", "authorization", "social", "api", "google", "facebook", "twitter"],
    "homepage": "https://github.com/mahekmanvar04/hybridauth",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Mahek",
            "email": "mahekpatel04@gmail.com"
        }
    ],
    "support": {
        "issues": "https://github.com/mahekmanvar04/hybridauth/issues"
    },
    "require": {
        "php": ">=5.4.0"
    },
    "require-dev": {
        "ext-curl": "*",
        "phpunit/phpunit": "~4.8.35"
